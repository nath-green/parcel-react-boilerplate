{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-dynamic-require": "warn",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "radix": ["error", "as-needed"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
